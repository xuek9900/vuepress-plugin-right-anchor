{
  "name": "vuepress-plugin-right-anchor",
  "version": "2.1.0-rc.1",
  "description": "Right-anchor plugin for Vuepress 2.x",
  "main": "lib/node/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rimraf lib *.tsbuildinfo",
    "copy": "cpx \"src/**/*.css\" lib",
    "prePublish": "npm run clean && npm run build && npm run copy",
    "build:example": "npm run build && rimraf example/right-anchor && npm run prePublish && cpx \"lib/**/*\" example/right-anchor/lib && cpx package.json example/right-anchor"
  },
  "keywords": [
    "vuepres",
    "right",
    "anchor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuekai-china/vuepress-plugin-right-anchor.git"
  },
  "author": "xuek",
  "license": "ISC",
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.45",
    "@vuepress/utils": "2.0.0-beta.45",
    "cpx2": "^4.1.2",
    "rimraf": "^3.0.2",
    "ts-debounce": "^4.0.0",
    "typescript": "^4.6.4",
    "vue": "^3.2.3",
    "vue-router": "^4.0.13"
  },
  "files": [
    "lib"
  ]
}
